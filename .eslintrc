{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-extra-semi": 0,
    "no-inner-declarations": 0,
    "no-cond-assign": 0,
    "no-empty": 0,
    "no-redeclare": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "comma-dangle": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [0, "single"],
    "jsx-quotes": 0,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "no-unreachable": 0,
    "no-native-reassign": 0,
    "no-unexpected-multiline": 0
  }
}
